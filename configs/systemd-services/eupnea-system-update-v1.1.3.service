[Unit]
Description=Eupnea update v1.1.3 postinstall script
# Further description:
# This script is only run once after the update to v1.1.3

[Service]
Type=oneshot
ExecStart=/usr/lib/eupnea/update-v1.1.3-postinstall.sh
RemainAfterExit=false
StandardOutput=journal

[Install]
WantedBy=multi-user.target